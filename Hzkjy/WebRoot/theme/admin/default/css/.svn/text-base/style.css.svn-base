/*表格项目样式*/
.ListTable {border-collapse: collapse; font-size: 13px;/*table-layout:fixed; width:100%;*/}
.ListTable  td{ color:#444; padding:5px 3px; border:1px solid #c9cdd1; word-wrap:break-word;}
.ListTable  td .inputbox{width:80%; border:1px solid #DEDEDE; height:20px; line-height:20px; padding-left:3px;color:#555; }
.ListTable  td .singleInputbox{width:15%; border:1px solid #DEDEDE; height:20px; line-height:20px; padding-left:3px;color:#555; }
.ListTable  td img{vertical-align:middle; cursor:pointer;}
.ListTable .tr{background-color:#E1F0FF;}
.ListTable  td a{color:#06c;text-decoration:none;}
.ListTable  td a:hover{ text-decoration:underline;}
.ListTable  .ListTableTitle{padding:8px 0px 8px 5px; background-color:#E1F0FF; color:#06c; font-weight:bold; text-align:left;border-bottom:2px solid #06c; }
.ListTable  .ListTableTitleNumList{padding:8px 5px 8px 0px; background-color:#E1F0FF; color:#0066CC; font-weight:bold; text-align:right;}
.ListTable  .NumList{padding:8px 5px 8px 0px; text-align:right;font-weight:bold;}
.ListTable  .ListTableTitle img{vertical-align:middle; cursor:pointer;}
.ListTable  .ListTableEnd{padding:12px 0px 12px 5px; background-color:#E1F0FF; color:#000000;border-top:2px solid #06c; }
.ListTable .tip_red{border-color:#F88;color:#F00;background-color:#FFDFDF;}
.ListTable .set_width{width:150px;background-color: #E1F0FF;font-weight: bold;text-align:right;color:#666;}

